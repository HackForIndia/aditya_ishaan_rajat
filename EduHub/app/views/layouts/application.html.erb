<!DOCTYPE html>
<html>
<head>
  <title>EduHub</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
	<%= link_to "Log out", logout_path, method: "delete" %>
<% end %>
<% if flash[:notice] %>
	<p><%= flash[:notice] %></p>
<% end %>
<%= yield %>

</body>
</html>
